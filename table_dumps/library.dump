CREATE TABLE Library (
    library_id SERIAL PRIMARY KEY,
    user_id INT REFERENCES Users(user_id),
    game_id INT REFERENCES Games(game_id),
    purchase_price DECIMAL(10,2),
    total_playtime INTEGER
);

